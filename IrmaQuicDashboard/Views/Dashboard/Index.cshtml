@{
    ViewData["Title"] = "Dashboard";
}
@model IrmaQuicDashboard.Models.ViewModels.SelectUploadSessionViewModel

<div class="text-center">
    <h1 class="display-4">Dashboard</h1>
</div>
<div class="d-flex justify-content-between">
    <div class="p-1"></div>
    <div class="p-1">
        <h3>Choose session:</h3>
        <form method="get" action="Dashboard">
            <select id="dropdownSelectSession" asp-for="UploadSessionId" asp-items="@Model.SessionSelection">
                <option>Please select one</option>
            </select>
            <input id="btnViewUploadSession" type="button" class="btn btn-primary" value="View Session" />
        </form>
    </div>
    <div class="p-1"></div>
</div>
<div id="dashboard" class="d-flex justify-content-between"></div>

<script src="~/js/site.js"></script>
